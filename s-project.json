{
  "name": "sobs",
  "custom": {
    "client": {
      "bucketName": "sobs.client.${stage}.${region}"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-client-s3",
    "serverless-cors-plugin"
  ]
}